{
  "name": "web-starter",
  "version": "0.5.7",
  "description": "Starter web application using node, express, handlebars, HTMX, nodemailer, libsql-orm and Tailwind",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=PROD node index.js",
    "dev": "NODE_ENV=DEV node --watch --env-file=.env index.js",
    "test": "jest",
    "generate": "topsecret generate --key-file config.key",
    "encrypt": "topsecret encrypt config.json config.secure --key-file config.key",
    "decrypt": "topsecret decrypt config.secure config.json --key-file config.key"
  },
  "dependencies": {
    "compression": "^1.7.5",
    "confirm-env": "^1.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "express-handlebars": "^8.0.1",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-tenants": "^0.7.8",
    "express-xss-sanitizer": "^2.0.0",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "html-minifier-terser": "^7.2.0",
    "https": "^1.0.0",
    "mongoose": "^8.11.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.16",
    "topsecret": "^0.7.3",
    "xss": "^1.0.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miketerry-org/web-starter.git"
  },
  "keywords": [
    "controller",
    "express",
    "express-tenants",
    "htmx",
    "libsql",
    "libsql-orm",
    "mvc",
    "model",
    "node",
    "nodemailer",
    "sqlite3",
    "tailwind",
    "view",
    "web"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "author": "miketerry.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miketerry-org/mvc-starter/issues"
  },
  "homepage": "https://github.com/miketerry-org/mvc-starter#readme",
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  }
}
